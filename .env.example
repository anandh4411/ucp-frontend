# Copy this file to .env and update the values

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_VERSION=/v1
VITE_API_TIMEOUT=10000
VITE_API_REFRESH_TIMEOUT=5000

# Auth Token Keys
VITE_ACCESS_TOKEN_KEY=impressaa_access_token
VITE_REFRESH_TOKEN_KEY=impressaa_refresh_token

# App Configuration  
VITE_APP_NAME=Impressaa
VITE_APP_VERSION=1.0.0

# Development Tools
VITE_ENABLE_DEV_TOOLS=true

# Set to true to disable auth guard (development only)
VITE_DISABLE_AUTH_GUARD=false

# Firebase Configuration (Get these from Firebase Console)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Cloudinary Configuration (FREE Storage Alternative - 25GB free!)
# Get these from: https://console.cloudinary.com/
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=unit_portal_uploads